    exit;


  with ZQ do
  begin
    Close;
    SQL.Clear;
    select := 'SELECT distinct a.* FROM Vehicle_Definition a ';

    if domain <> -1 then
    begin
      where := 'WHERE a.Platform_Domain = '+ IntToStr(domain);
