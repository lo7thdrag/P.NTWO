        begin
          Vehicle_Index       := FieldByName('Vehicle_Index').AsInteger;
          Vehicle_Identifier  := FieldByName('Vehicle_Identifier').AsString;
        end;

        pList.Add(rec);
        ZQ.Next;
      end;
    end;
  end;
