    SQL.Add('WHERE (Gun_Index = ' +  IntToStr(id) + ')' );
    if tipe <> 0 then
      SQL.Add('AND (Target_Type = ' +  IntToStr(tipe) + ')' );
    SQL.Add('ORDER BY Range ASC ' );
    Open;

    result := RecordCount;
    if not IsEmpty then begin

